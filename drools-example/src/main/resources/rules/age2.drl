package com.example.demo02

rule "age2-1"
    when
        $d : Double() from entry-point "age-gate1";
    then
        System.err.println("age2.drl gate1 : " + $d);
        kcontext.getKieRuntime().getEntryPoint("gate2").insert(3d+$d);
end

rule "age2-2"
    when
        $d : Double() from entry-point "age-gate2";
    then
        System.err.println("age2.drl gate2 : " + $d);
end